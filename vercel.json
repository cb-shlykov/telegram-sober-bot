{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/bot.js"
    },
    {
      "src": "/cron",
      "dest": "/api/cron.js"
    },
    {
      "src": "/test",
      "dest": "/api/test.js"
    }
  ]
}
